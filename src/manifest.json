{
    "manifest_version": 2,
    "name": "深度论坛主题切换",
    "version": "0.0.1",
    "description": "深度论坛主题切换",
    "icons": {
        "16": "img/icon.png",
        "48": "img/icon.png",
        "128": "img/icon.png"
    },
    "permissions": [
        "declarativeContent",
        "tabs",
        "http://*/*",
        "https://*/*",
        "storage"
    ],
    "web_accessible_resources": [
        "css/dark.css"
    ],
    "background": {
        "scripts": [
            "js/background.js"
        ]
    },
    "homepage_url": "https://gitee.com/hlbj105/deepin-forum-theme-toggle",
    "page_action": {
        "default_icon": "img/icon.png",
        "default_title": "深度论坛主题切换",
        "default_popup": "html/popup.html"
    }
}